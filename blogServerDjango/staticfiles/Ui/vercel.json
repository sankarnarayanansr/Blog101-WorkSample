{
  "version": 2,
 
    "buildCommand": "echo 'âœ… Vercel buildCommand is working!'",
   
  
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
        
      }
    },
    {
      "src": "blogServerDjango/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    }, 
    {
   
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Ui/build/index.html"
    }
  ]
  
} 