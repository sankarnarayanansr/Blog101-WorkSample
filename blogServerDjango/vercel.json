{
  "version": 2,
  "builds": [
    
 
    {
      "src": "blogServerDjango/wsgi.py",
      "use": "@vercel/python"
    }
  ],
 "buildCommand": "bash build.sh",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "blogServerDjango/wsgi.py"
    }
  ] ,
  "build": {
    
    "commands": [
      "cd static/Ui && npm install && npm run build"
    ]
  } 
}
